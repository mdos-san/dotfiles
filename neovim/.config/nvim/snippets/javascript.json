{
  "Variable": {
    "prefix": ["let"],
    "body": ["let ${1: varName} = ${2:value}"]
  },
  "Constant": {
    "prefix": ["con"],
    "body": ["const ${1: varName} = ${2:value}"]
  },
  "If": {
    "prefix": ["if"],
    "body": ["if (${1:0 < 1}) {", "  ${2:// TODO}", "}"]
  },
  "While": {
    "prefix": ["whi"],
    "body": ["while (${1:0 < 1}) {", "  ${2:// TODO}", "}"]
  },
  "For": {
    "prefix": ["for"],
    "body": ["for (let ${1:i = 0}, ${2:i < 42}, ${3:++i}) {", "  ${0:// TODO}", "}"]
  },
  "Function": {
    "prefix": ["fun"],
    "body": ["function ${1:functionName}(${2:a, b}) {", "  ${0:// TODO}", "}"]
  },
  "Arrow function": {
    "prefix": ["=>"],
    "body": ["(${1:a, b}) => {", "  ${2:// TODO}", "}"]
  },
  "React functionnal component": {
    "prefix": ["rea"],
    "body": [
      "import React from 'react'",
      "",
      "const ${1:Component} = () => {",
      "  return ${2:// JSX}",
      "}",
      "",
      "export default ${1:Component}"
    ]
  },
  "JSX Tag": {
    "prefix": ["<<"],
    "body": ["<${1:TagName}>", "  ${0:// Child}", "</${1:TagName}>"]
  }
}
