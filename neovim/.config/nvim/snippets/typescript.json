{
  "If condition": {
    "prefix": ["iff"],
    "body": ["if (${1:0 < 1}) {", "  ${2:// Some code ...}", "}"]
  },
  "While loop": {
    "prefix": ["whi"],
    "body": ["while (${1:0 < 1}) {", "  ${2:// Some code ...}", "}"]
  },
  "Anonymous arrow function": {
    "prefix": ["=>"],
    "body": ["(${1:a, b}) => {", "  ${2:// Some code ...}", "}"]
  },
  "Named arrow function": {
    "prefix": ["==>"],
    "body": [
      "const ${1:functionName} = (${2:a, b}) => {",
      "  ${3:// Some code ...}",
      "}"
    ]
  },
  "React functionnal component": {
    "prefix": ["rea"],
    "body": [
      "import React from 'react'",
      "",
      "interface ${1:Component}Props {",
      "}",
      "",
      "const ${1:Component} = ({}: ${1:Component}Props): JSX.Element => {",
      "  return ${2:// JSX}",
      "}",
      "",
      "export default ${1:Component}"
    ]
  },
  "JSX Tag": {
    "prefix": ["<<"],
    "body": [
      "<${1:TagName}>",
      "  ${0:// Child}",
      "</${1:TagName}>"
    ]
  }
}
